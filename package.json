    "eslint-plugin-svelte": "^3.0.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "mdx-svelte": "^5.0.0",
    "prettier": "~3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
